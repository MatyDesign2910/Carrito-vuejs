 <p class=\"menu-label\">Categorías</p>\n    <ul class=\"menu-list\" >\n      <li><button class=\"a-1\" type=\"button\" v-on:click=\"activeUno()\">Categoría 1</button></li>\n      <li><button class=\"a-2\" type=\"button\" v-on:click=\"activeDos()\">Categoría 2</button></li>\n    </ul>\n  </aside>\n</template>\n<script>\nimport { mapGetters } from \"vuex\";\nexport default {\n  el: \"#app\",\n  name: \"Navbar\",\n  data() {\n    return {\n      searchQuery: \"\",\n    };\n  },\n  methods: {\n    activeUno: function() {\n      $('.categoria-2').addClass('d-none');\n      $('.categoria-1').removeClass('d-none');\n      $('.a-1').addClass('is-active');\n      $('.a-2').removeClass('is-active');\n    },\n    activeDos: function() {\n      $('.categoria-1').addClass('d-none');\n      $('.categoria-2').removeClass('d-none');\n      $('.a-2').addClass('is-active');\n      $('.a-1').removeClass('is-active');\n    }\n  },\n  computed: {\n    filteredResources() {\n      if (this.searchQuery) {\n        return this.productItems.filter((productItem) => {\n          return productItem.title.startsWith(this.searchQuery);\n        });\n      } else {\n        return this.productItems;\n      }\n    },\n    ...mapGetters([\"productItems\"]),\n  },\n  created() {\n    this.$store.dispatch(\"getProductItems\");\n  },\n};\n</script>\n\n<style scoped>\n  .d-none {\n    display:none !important;\n  }\n</style>"]}]}