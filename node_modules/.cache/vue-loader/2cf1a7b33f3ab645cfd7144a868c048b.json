/volumes/root/htdocs/carrito-vuejs/src/components/product/Product_List_Item.vue","sourceRoot":"","sourcesContent":["<template>\n    <div class=\"card\">\n        <div class=\"card-image\">\n            <figure class=\"image is-4by3\">\n                <img :src=\"productItem.image_tag\" class=\"img-carrito\" alt=\"Placeholder image\">\n            </figure>\n        </div>\n      <div class=\"card-content\">\n        <div class=\"content\">\n          <h4>{{ productItem.title }}</h4>\n          <p class=\"mt-4\">\n            <!--{{ productItem.description }}-->\n          </p>\n        </div>\n        <div class=\"media\">\n          <div class=\"media-content\">\n            <p class=\"title is-6\">{{ productItem.owner }}</p>\n            <p class=\"subtitle is-7\">{{ productItem.email }}</p>\n          </div>\n          <div class=\"media-right\">\n            <a class=\"button is-primary is-light\">\n              <strong>$ {{ productItem.price }}</strong>\n            </a>\n          </div>\n          <a\n            class=\"button is-rounded is-pulled-left\"\n            @click=\"addCartItem(productItem)\"\n          >\n            <strong><i class=\"fa fa-2x fa-shopping-cart\"></i></strong>\n          </a>\n        </div>\n      </div>\n    </div>\n</template>\n<script>\nimport {mapActions} from 'vuex'\nexport default {\n  name: \"ProductListItem\",\n  props: [\"productItem\"],\n  methods: {\n    ...mapActions([\"addCartItem\"]),\n  },\n};\n</script>"]}]}